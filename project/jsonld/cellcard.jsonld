      "mixin": true,
      "domain": "Database",
      "slot_uri": "https://cellcards.org/object_set",
      "multivalued": true,
      "inlined_as_list": true,
      "owner": "object_set",
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "cell_card_set",
      "definition_uri": "https://cellcards.org/cell_card_set",
